{
  "name": "web-baseline",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.6.3",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@antfu/ni": "^24.2.0",
    "@types/node": "^22.13.10",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "nano-staged": "^0.8.0",
    "ofetch": "^1.4.1",
    "simple-git-hooks": "^2.11.1",
    "tinyexec": "^0.3.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.8",
    "vitest-package-exports": "^0.1.1",
    "zod": "^3.24.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm i --frozen-lockfile --ignore-scripts --offline && npx nano-staged"
  },
  "nano-staged": {
    "*": "eslint --fix"
  }
}
